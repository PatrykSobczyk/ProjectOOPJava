<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="400.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.112" fx:controller="person.CustomerController">
    <children>
        <ImageView fx:id="ImageCustomer" fitHeight="200.0" fitWidth="200.0" layoutX="14.0" layoutY="25.0"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/companycustomerbig.png"/>
            </image>
        </ImageView>
        <Label layoutX="500.0" layoutY="5.0" text="Info">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <ListView fx:id="CustomerInfo" layoutX="250.0" layoutY="30.0" prefHeight="300.0" prefWidth="500.0"/>
        <Button layoutX="82.0" layoutY="268.0" mnemonicParsing="false" text="Delete" onMouseClicked="#removeClients"/>

    </children>
</AnchorPane>
